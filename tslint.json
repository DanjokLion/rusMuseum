{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [ true ],
        "no-shadowed-variable": false,
        "no-unused-expression": false,
        "no-console": false,
        "no-empty": false,
        "new-parens": false,
        "no-var-requires": false,
        "variable-name": false,
        "triple-equals": false,
        "only-arrow-functions": false
    },
    "rulesDirectory": []
}
